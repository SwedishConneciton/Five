{
  "Data": {
    "DefaultConnection": {
      "ConnectionString": "Host=192.168.99.100;Username=postgres;Password=Admin;Database=postgres"
    },

    "IdentityFramework": {
      "ConnectionString": "Host=192.168.99.100;Username=postgres;Password=Admin;Database=whoareyou"
    }
  },

  "EntityFramework": {
    "ApplicationDbContext": {
      "ConnectionStringKey": "Data:DefaultConnection:ConnectionString"
    },

    "IdentityDbContext": {
      "ConnectionStringKey": "Data:IdentityFramework:ConnectionString"
    }
  },

  "Identity": {
    "Password": {
      "RequireDigit": "false",
      "RequireLowercase": "false",
      "RequiredLength": "6",
      "RequireUppercase": "false",
      "RequireNonLetterOrDigit": "false"
    }
  },

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Verbose",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "OpenId": {
    "Audience": "http://localhost:5000/",
    "Authority": "http://localhost:5000/"
  }
}
